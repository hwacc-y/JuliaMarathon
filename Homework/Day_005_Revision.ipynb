   "source": [
    "count = 0\n",
    "for i = 1:100000\n",
    "    x = rand() \n",
    "    y = rand()\n",
    "    \n",
    "    # 計算隨機點到中心點的距離，若距離小於半徑，表示點落在圓內\n",
    "    dist = sqrt((x - 0.5) ^ 2 + (y - 0.5) ^ 2)\n",
    "    \n",
    "    if dist <= 0.5\n",
    "        count += 1\n",
    "    end\n",
    "end\n",
    "mypi = 4 * (count/100000)\n",
    "println(mypi)"
   ]
